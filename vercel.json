{
    "version": 2,
    "builds": [
        {
            "src": "website/website.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "/(auth|api|admin)/.*",
            "dest": "website/website.js"
        },
        {
            "src": "/(.*).html",
            "dest": "/public/$1.html"
        },
        {
            "src": "/",
            "dest": "website/website.js"
        }
    ]
}